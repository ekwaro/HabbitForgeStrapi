{
  "kind": "collectionType",
  "collectionName": "feedbacks",
  "info": {
    "singularName": "feedback",
    "pluralName": "feedbacks",
    "displayName": "Feedback",
    "description": "Feedback from partner"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "message": {
      "type": "text",
      "required": true
    },
    "goal": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::goal.goal"
    },
    "habit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::habit.habit"
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5
    },

    "sender": {

      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "receiver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"

    }
  }
}
