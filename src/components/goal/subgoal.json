{
  "collectionName": "components_goal_subgoals",
  "info": {
    "displayName": "Subgoal",
    "icon": "check-circle"
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "progress": {
      "type": "integer",
      "min": 0,
      "max": 100,
      "default": 0
    },
    "priority": {
      "type": "enumeration",
      "enum": ["Low", "Medium", "High"],
      "default": "Medium"
    },
    "completed": {
      "type": "boolean",
      "default": false
    }
  }
}
